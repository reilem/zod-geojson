{
    "name": "zod-geojson",
    "version": "1.0.0",
    "description": "Zod schemas for GeoJSON",
    "repository": {
        "type": "git",
        "url": "https://github.com/reilem/zod-geojson"
    },
    "author": "Reinert Lemmens",
    "license": "MIT",
    "keywords": [
        "zod",
        "geojson",
        "schema",
        "validation"
    ],
    "main": "lib/cjs/index.js",
    "module": "lib/esm/index.js",
    "types": "lib/types/index.d.ts",
    "exports": {
        ".": {
            "types": "./dist/types/index.d.ts",
            "import": "./dist/esm/index.js",
            "require": "./dist/cjs/index.js",
            "default": "./dist/esm/index.js"
        }
    },
    "sideEffects": false,
    "packageManager": "pnpm@8.10.4",
    "files": [
        "lib",
        "README.md",
        "LICENSE"
    ],
    "scripts": {
        "test": "jest",
        "clean": "rimraf lib",
        "build": "pnpm clean && pnpm build:commonjs && pnpm build:esm && pnpm build:types",
        "build:commonjs": "tsc -p tsconfig.build.cjs.json",
        "build:esm": "tsc -p tsconfig.build.esm.json",
        "build:types": "tsc -p tsconfig.build.json --emitDeclarationOnly",
        "typecheck": "tsc",
        "prettiercheck": "prettier -c .",
        "circularcheck": "madge --circular --extensions ts ./src"
    },
    "devDependencies": {
        "@jest/globals": "^29.7.0",
        "@types/geojson": "^7946.0.14",
        "jest": "^29.7.0",
        "madge": "^8.0.0",
        "prettier": "^3.2.5",
        "rimraf": "^5.0.5",
        "ts-jest": "^29.1.2",
        "typescript": "~5.8.3",
        "zod": "~3.25.64"
    }
}
